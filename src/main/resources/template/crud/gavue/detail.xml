<?xml version="1.0" encoding="utf-8"?>
<template>
	<name>ga_vue_js</name>
	<filePath>genResult/pages/views/service/index/children/st/</filePath>
	<fileName>${table.name}-detail.vue</fileName>
	<content><![CDATA[
<!--${functionName}管理-->
<template>
  <div class="wrapper">
    <div class="header-wrap">
      <div class="back" @click="returnBack()">
        <i class="el-icon-arrow-left" />返回
      </div>
      <div class="title">${functionName}</div>
    </div>

    <div class="body">
      <div class="form-wrap">
        <el-form
          ref="applyRef1"
          class="dark-form"
          :model="applyForm"
          label-width="110px"
        >
  <#list table.columnList as c>
  	<#if c.uiControl?? >
          <el-form-item label="${c.comments}" prop="${c.javaField}">
            <el-input
              v-model="applyForm.${c.javaField}"
              disabled
              type="textarea"
              :rows="5"
              placeholder="${c.comments}"
            />
          </el-form-item>
 	</#if>
  </#list>

          <el-form-item style="display: flex; justify-content: center">
            <el-button class="gray-btn" @click="returnBack()">返回</el-button>
          </el-form-item>
        </el-form>
      </div>
      <div />
    </div>
      </div>
      <span slot="footer">
        <el-button
          size="small"
          @click="submitDialogVisible = false"
        >返回</el-button>
        <el-button size="small" @click="submit()">确定</el-button>
      </span>
    </el-dialog>
  </div>
</template>


]]>
	</content>
</template>