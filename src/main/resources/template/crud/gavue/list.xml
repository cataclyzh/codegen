<?xml version="1.0" encoding="utf-8"?>
<template>
	<name>ga_vue_js</name>
	<filePath>genResult/pages/views/service/index/children/st/</filePath>
	<fileName>${table.name}-list.vue</fileName>
	<content><![CDATA[
<!--${functionName}管理-->

<template>
  <div class="chart-wrapper">
    <div class="chart-inner">
      <div class="table-wrap">
        <el-table
          ref="tableRef"
          class="circle border"
          :data="tableData"
          stripe
          :highlight-current-row="true"
        >
          <el-table-column label="序号" prop="id" type="index" width="100" :index="indexMethod" />
  <#list table.columnList as c>
          <el-table-column label="${c.comments}" align="center" prop="${c.javaField}" />
  </#list>
          <el-table-column label="操作" width="400" align="center">
            <template slot-scope="scope">
              <el-button type="default" size="small" @click="viewData(scope.row)">查看</el-button>
              <el-button type="primary" size="small" plain :disabled="!scope.row.isEditable" @click="updateData(scope.row)">编辑</el-button>
              <el-button type="danger" size="small" plain :disabled="!scope.row.isDeletable" @click="deleteData(scope.row.id)">删除</el-button>
              <!--<el-button type="primary" size="small" :disabled="!scope.row.isPushable" @click="transferData(scope.row)">-->
                <!--推送-->
              <!--</el-button>-->
            </template>
          </el-table-column>
        </el-table>
      </div>
    </div>
  </div>
</template>

<script>
export default {
  props: {
    tableData: {
      type: Array,
      default() {
        return []
      }
    },
    title: {
      type: String,
      default: ''
    },
    page: {
      type: [Number, String],
      default: 1
    },
    pageSize: {
      type: [Number, String],
      default: 10
    }
  },
  data() {
    return {
      userInfo: '', disabled: false
    }
  },
  created() {
    this.userInfo = this.$store.state.user.userInfo.organization.type
  },
  methods: {
    // 删除
    deleteData(row) {
      this.$emit('deleteData', row)
    },
    // 编辑
    updateData(row) {
      this.$emit('updateData', row)
    },
    // 查看
    viewData(row) {
      this.$emit('viewData', row)
    },
    indexMethod(index) {
      index = (this.page - 1) * this.pageSize + index + 1
      return index
    }
  }
}
</script>

<style lang="scss" scoped>
</style>


]]>
	</content>
</template>